				{
					"Type": "NodeCodeBlockCode",
					"Data": "version: '3.8'\nservices:\n  oxidized:\n    image: oxidized/oxidized:latest  # 官方镜像，也可使用私仓镜像\n    container_name: oxidized\n    restart: always  # 容器异常自动重启\n    network_mode: host  # 宿主机网络模式（推荐，便于访问设备）\n    volumes:\n      # 持久化配置文件\n      - /data/oxidized/config:/root/.config/oxidized\n      # 持久化设备数据库/备份文件\n      - /data/oxidized/data:/root/.config/oxidized/data\n      # 持久化日志\n      - /data/oxidized/logs:/var/log/oxidized\n      # 持久化配置备份文件\n      - /data/oxidized/backup:/backup\n    environment:\n      # 同步宿主机时区，避免日志时间错乱\n      - TZ=Asia/Shanghai\n      # 自定义Oxidized配置路径（可选）\n      - OXIDIZED_HOME=/root/.config/oxidized\n    # 端口映射（host模式下可注释，非host模式需开放8888端口）\n    # ports:\n    #   - \"8888:8888\"\n    # 启动命令（默认启动web服务+备份进程）\n    command: [\"oxidized\", \"-d\"]version: '3.8'\nservices:\n  oxidized:\n    image: oxidized/oxidized:latest  # 官方镜像，也可使用私仓镜像\n    container_name: oxidized\n    restart: always  # 容器异常自动重启\n    network_mode: host  # 宿主机网络模式（推荐，便于访问设备）\n    volumes:\n      # 持久化配置文件\n      - /data/oxidized/config:/root/.config/oxidized\n      # 持久化设备数据库/备份文件\n      - /data/oxidized/data:/root/.config/oxidized/data\n      # 持久化日志\n      - /data/oxidized/logs:/var/log/oxidized\n      # 持久化配置备份文件\n      - /data/oxidized/backup:/backup\n    environment:\n      # 同步宿主机时区，避免日志时间错乱\n      - TZ=Asia/Shanghai\n      # 自定义Oxidized配置路径（可选）\n      - OXIDIZED_HOME=/root/.config/oxidized\n    # 端口映射（host模式下可注释，非host模式需开放8888端口）\n    # ports:\n    #   - \"8888:8888\"\n    # 启动命令（默认启动web服务+备份进程）\n    command: [\"oxidized\", \"-d\"]version: '3.8'\nservices:\n  oxidized:\n    image: oxidized/oxidized:latest  # 官方镜像，也可使用私仓镜像\n    container_name: oxidized\n    restart: always  # 容器异常自动重启\n    network_mode: host  # 宿主机网络模式（推荐，便于访问设备）\n    volumes:\n      # 持久化配置文件\n      - /data/oxidized/config:/root/.config/oxidized\n      # 持久化设备数据库/备份文件\n      - /data/oxidized/data:/root/.config/oxidized/data\n      # 持久化日志\n      - /data/oxidized/logs:/var/log/oxidized\n      # 持久化配置备份文件\n      - /data/oxidized/backup:/backup\n    environment:\n      # 同步宿主机时区，避免日志时间错乱\n      - TZ=Asia/Shanghai\n      # 自定义Oxidized配置路径（可选）\n      - OXIDIZED_HOME=/root/.config/oxidized\n    # 端口映射（host模式下可注释，非host模式需开放8888端口）\n    # ports:\n    #   - \"8888:8888\"\n    # 启动命令（默认启动web服务+备份进程）\n    command: [\"oxidized\", \"-d\"]\n"
