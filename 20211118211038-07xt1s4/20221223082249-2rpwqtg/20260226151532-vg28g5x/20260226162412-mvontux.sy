de 93 = unsigned integer 16;\n\nsubnet 192.168.255.0 netmask 255.255.255.0 {\n  range 192.168.255.100 192.168.255.200;\n  option domain-name-servers pxe.example.com;\n  option domain-name \"example.com\";\n  option routers 192.168.255.254;\n  class \"pxeclients\" {\n    match if substring (option vendor-class-identifier, 0, 9) = \"PXEClient\";\n    next-server 192.168.255.254;\n          if option architecture-type = 00:07 {\n            filename \"UEFI/BOOTX64.EFI\";\n          }\n          else {\n            filename \"BIOS/pxelinux.0\";\n          }\n  }\n  #class \"httpclients\" {\n  #  match if substring (option vendor-class-identifier, 0, 10) = \"HTTPClient\";\n  #  option vendor-class-identifier \"HTTPClient\";\n  #  filename \"http://192.168.124.2/redhat/EFI/BOOT/BOOTX64.EFI\";\n  #}\n}\nEOF\n"},{"Type":"NodeCodeBlockFenceCloseMarker","Data":"```"}]},{"ID":"20260226162412-ap4mcjx","Type":"NodeHeading","HeadingLevel":2,"Properties":{"id":"20260226162412-ap4mcjx","updated":"20260226162412"},"Children":[{"Type":"NodeText","Data":"准备 HTTP 服务"}]},{"ID":"20260226162412-b8gqy42","Type":"NodeHeading","HeadingLevel":2,"Properties":{"id":"20260226162412-b8gqy42","updated":"20260226162412"},"Children":[{"Type":"NodeText","Data":"安装 HTTP 服务"}]},{"ID":"20260226162412-ajejqil","Type":"NodeCodeBlock","IsFencedCodeBlock":true,"Properties":{"id":"20260226162412-ajejqil","updated":"20260226162412"},"Children":[{"Type":"NodeCodeBlockFenceOpenMarker","Data":"```"},{"Type":"NodeCodeBlockFenceInfoMarker"},{"Type":"NodeCodeBlockCode","Data":"[root@pxe ~]# dnf -y install httpd\n"},{"Type":"NodeCodeBlockFenceCloseMarker","Data":"```"}]},{"ID":"20260226162412-l5jopii","Type":"NodeHeading","HeadingLevel":2,"Properties":{"id":"20260226162412-l5jopii","updated":"20260226162412"},"Children":[{"Type":"NodeText","Data":"制作镜像源和 Kickstart 文件源"}]},{"ID":"20260226162412-4h5kp1w","Type":"NodeHeading","HeadingLevel":3,"Properties":{"id":"20260226162412-4h5kp1w","updated":"20260226162412"},"Chil