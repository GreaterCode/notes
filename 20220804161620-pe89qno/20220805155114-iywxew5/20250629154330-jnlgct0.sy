			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "1） 检查 Etcd 服务状态："
				}
			]
		},
		{
			"ID": "20250629154330-dyqr2wg",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-dyqr2wg",
				"updated": "20250629154621"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 在 Etcd 节点上使用 "
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "systemctl status etcd"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": " (系统服务) 或 "
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "docker ps | grep etcd"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": " (容器) 检查运行状态。"
				}
			]
		},
		{
			"ID": "20250629154330-03u5nz7",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-03u5nz7",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 若服务停止，尝试重启："
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "systemctl restart etcd"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": " 或 "
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "docker restart etcd"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "。"
				}
			]
		},
		{
			"ID": "20250629154330-5177ljq",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-5177ljq",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "2）查看日志："
				}
			]
		},
		{
			"ID": "20250629154330-6x37dmm",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-6x37dmm",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 使用 "
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "journalctl -u etcd"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": " (系统服务) 或 "
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "docker logs etcd"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": " (容器) 检查日志，定位具体错误信息。"
				}
			]
		},
		{
			"ID": "20250629154330-1axte94",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-1axte94",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "3）验证集群健康状态："
				}
			]
		},
		{
			"ID": "20250629154330-9vlxzy5",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-9vlxzy5",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 使用 "
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "etcdctl endpoint health"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": " 命令检查集群健康。"
				}
			]
		},
		{
			"ID": "20250629154330-zqu47f0",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-zqu47f0",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 若 Etcd 运行在 Kubernetes Pod 内，需先进入 Pod ("
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "kubectl exec -it  -- sh"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": ") 再执行命令。"
				}
			]
		},
		{
			"ID": "20250629154330-qj5fhb6",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-qj5fhb6",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "4）恢复 Etcd 数据 (针对数据丢失/损坏)："
				}
			]
		},
		{
			"ID": "20250629154330-zzo6erj",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-zzo6erj",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 使用 "
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "etcdctl snapshot restore /path/to/snapshot.db --data-dir /var/lib/etcd"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": " 命令从备份快照恢复数据。"
				}
			]
		},
		{
			"ID": "20250629154330-dodas9a",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-dodas9a",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 关键前提：必须有可用的备份快照。"
				}
			]
		},
		{
			"ID": "20250629154330-7i9drm3",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-7i9drm3",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 恢复后需重启 Etcd 服务。"
				}
			]
		},
		{
			"ID": "20250629154330-p1vs6k3",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-p1vs6k3",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "5）重新加入集群 (针对集群状态异常)："
				}
			]
		},
		{
			"ID": "20250629154330-z705oyp",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20250629154330-z705oyp",
				"updated": "20250629154330"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "-- 添加新节点："
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
					]
				},
				{
					"Type": "NodeText",
					"Data": "etcdctl member add  --peer-urls"
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "="
						}
					]
				},
				{
					"Type": "NodeBackslash",
					"Data": "span",
					"Children": [
						{
							"Type": "NodeText",
							"Data": "`"
						}
