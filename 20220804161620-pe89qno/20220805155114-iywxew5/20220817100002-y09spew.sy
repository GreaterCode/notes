			"Properties": {
				"id": "20220817100002-duupcqq"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "cat \u003eca-csr.json \u003c\u003cEOF\n{\n  \"CN\": \"etcd\",\n  \"key\": {\n    \"algo\": \"rsa\",\n    \"size\": 2048\n  },\n  \"names\": [\n    {\n      \"C\": \"CN\",\n      \"ST\": \"shanghai\",\n      \"L\": \"shanghai\",\n      \"O\": \"etcd\",\n      \"OU\": \"System\"\n    }\n  ]\n}\nEOF"
